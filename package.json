{
    "name": "pybc",
    "displayName": "pybc",
    "description": "A simple extension for python bytecode syntax highlighting.",
    "version": "0.0.1",
    "publisher": "msm",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pybc",
            "aliases": ["Python Bytecode"],
            "extensions": [".pybc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pybc",
            "scopeName": "source.pybc",
            "path": "./syntaxes/pybc.tmLanguage.json"
        }]
    }
}
